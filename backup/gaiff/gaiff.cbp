<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gaiff" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="..\gaudio-sdk\bin\addons\gaiff" prefix_auto="1" extension_auto="1" />
				<Option object_output="C:\Documents and Settings\Administrator\My Documents" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add directory="..\gaudio-sdk\include" />
					<Add directory="..\gaudio" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="..\gaudio-sdk\lib\libgaudio.dll.a" />
				</Linker>
			</Target>
		</Build>
		<Linker>
			<Add option="-static-libgcc" />
			<Add option="-static-libstdc++" />
		</Linker>
		<Unit filename="gaiff.cpp" />
		<Unit filename="private.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
